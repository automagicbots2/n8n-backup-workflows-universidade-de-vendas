    {
      "parameters": {
        "jsCode": "const formResponse = $node[\"cadastro_na_lp\"].json[\"body\"][\"form_response\"];\nconst hidden = formResponse.hidden || {};\nconst answers = formResponse.answers || [];\n\n// Captura dados ocultos\nconst fullNameAnswer = hidden.name || \"\";\nconst emailAnswer = hidden.email || \"\";\nconst telefoneClienteOriginal = hidden.phone || \"\";\n\n// Captura UTMs\nconst utm_source = hidden.utm_source || \"\";\nconst utm_medium = hidden.utm_medium || \"\";\nconst utm_campaign = hidden.utm_campaign || \"\";\nconst utm_term = hidden.utm_term || \"\";\nconst utm_content = hidden.utm_content || \"\";\nconst utm_id = hidden.utm_id || \"\";\n\n// Fun√ß√µes auxiliares\nfunction capitalizeName(fullName) {\n  if (!fullName) return { fname: '', lname: '', fullname: '' };\n\n  const lowerCaseWords = ['da', 'de', 'do', 'di', 'du'];\n  let words = fullName.toLowerCase().split(' ').map((word, index) => {\n    if (lowerCaseWords.includes(word) && index !== 0) return word;\n    return word.charAt(0).toUpperCase() + word.slice(1);\n  });\n\n  const fname = words.shift();\n  const lname = words.join(' ');\n  return { fname, lname, fullname: `${fname} ${lname}` };\n}\n\nfunction formatEmail(email) {\n  return email.toLowerCase().trim();\n}\n\nfunction formatInstagram(instagram) {\n  return instagram.toLowerCase().replace(/^@/, \"\").trim();\n}\n\nfunction formatCPF(cpf) {\n  const cpfLimpo = cpf.replace(/\\D/g, \"\");\n  return cpfLimpo.length === 11\n    ? `${cpfLimpo.slice(0, 3)}.${cpfLimpo.slice(3, 6)}.${cpfLimpo.slice(6, 9)}-${cpfLimpo.slice(9)}`\n    : cpf;\n}\n\nfunction formatCNPJ(cnpj) {\n  const cnpjLimpo = cnpj.replace(/\\D/g, \"\");\n  return cnpjLimpo.length === 14\n    ? `${cnpjLimpo.slice(0, 2)}.${cnpjLimpo.slice(2, 5)}.${cnpjLimpo.slice(5, 8)}/${cnpjLimpo.slice(8, 12)}-${cnpjLimpo.slice(12)}`\n    : cnpj;\n}\n\n// Formata√ß√£o de telefone\nlet telefoneFinal = \"\";\nif (telefoneClienteOriginal) {\n  const telefoneLimpo = telefoneClienteOriginal.replace(/\\D/g, \"\");\n\n  let DDI = '', DDD = '', numero = '';\n  if (telefoneLimpo.length > 11) {\n    DDI = telefoneLimpo.slice(0, 2);\n    DDD = telefoneLimpo.slice(2, 4);\n    numero = telefoneLimpo.slice(4);\n  } else if (telefoneLimpo.length === 11 || telefoneLimpo.length === 10) {\n    DDI = '55';\n    DDD = telefoneLimpo.slice(0, 2);\n    numero = telefoneLimpo.slice(2);\n  } else {\n    telefoneFinal = telefoneLimpo;\n  }\n\n  if (DDD && numero) {\n    if (parseInt(DDD) > 28 && numero.length === 9 && numero.startsWith(\"9\")) {\n      numero = numero.slice(1);\n    } else if (numero.length === 8) {\n      numero = `9${numero}`;\n    }\n  }\n\n  if (DDI && DDD && numero) {\n    telefoneFinal = `${DDI}${DDD}${numero}`;\n  }\n}\n\n// Dados padr√£o\nconst cpfAnswer = \"\";\nconst cnpjAnswer = \"\";\nconst formName = formResponse.definition.title || \"Formul√°rio\";\nconst formId = formResponse.form_id || \"desconhecido\";\n\n// üîπ Mapeia perguntas e respostas com o `ref` da resposta como chave\nconst perguntas_respostas = {};\n\nfor (const answer of answers) {\n  let resposta = \"\";\n  let idResposta = null;\n\n  if (answer.type === \"choice\" && answer.choice) {\n    resposta = answer.choice.label;\n    idResposta = answer.choice.ref;\n  }\n\n  if (answer.type === \"boolean\") {\n    resposta = answer.boolean ? \"Sim\" : \"N√£o\";\n    idResposta = answer.field.ref;\n  }\n\n  if (answer.type === \"text\") {\n    resposta = answer.text;\n    idResposta = answer.field.ref;\n  }\n\n  if (idResposta) {\n    perguntas_respostas[idResposta] = resposta;\n  }\n}\n\n// Formata√ß√µes finais\nconst formattedNames = capitalizeName(fullNameAnswer);\nconst formattedEmail = formatEmail(emailAnswer);\nconst formattedInstagram = formatInstagram(perguntas_respostas[\"bae8dbfc-3bc4-4b25-b431-4d32910b51eb\"] || \"\");\n\nconst result = {\n  ...formattedNames,\n  email: formattedEmail,\n  whatsapp: telefoneFinal,\n  instagram: formattedInstagram,\n  cpf: formatCPF(cpfAnswer),\n  cnpj: formatCNPJ(cnpjAnswer),\n  form_name: formName,\n  form_id: formId,\n  utm_source,\n  utm_medium,\n  utm_campaign,\n  utm_term,\n  utm_content,\n  utm_id,\n  perguntas_respostas\n};\n\nreturn [{ json: result }];\n"
      },
