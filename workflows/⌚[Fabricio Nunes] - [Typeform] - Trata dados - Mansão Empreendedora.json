        "jsCode": "// 東 Captura os dados do input\nconst inputData = items[0].json;\nconst formResponse = inputData.body.form_response;\nconst respostas = formResponse.answers || [];\n\n// 東 Mapeamento por REF\nconst perguntasMap = {\n  \"c7e48309-1689-4506-a993-9799645b475c\": \"fullName\",\n  \"dee1b690-bc80-450e-9734-bd69b63ebcc7\": \"email\",\n  \"931dc05a-435e-4e39-8e30-f83152803e3e\": \"telefone\",\n  \"f8728f50-a5f0-4d24-b6db-3435bbab0218\": \"nome_empresa\",\n  \"6534f24e-25d6-4eee-9a17-521eef31a1a4\": \"instagram\",\n  \"892f7bb0-8b06-401b-a54c-5c8f0f2b3c92\": \"dificuldades_empresa\",\n  \"c00874cc-a02d-4622-8ba1-5eb6922322b4\": \"faturamento\",\n  \"139fada5-f083-47ce-81df-a3dc76a86d5b\": \"metricas_claras\",\n  \"8a6a4ac5-eb2d-4683-bda2-d9679a685aca\": \"tem_conselho\",\n  \"2ffe5f89-ee54-4ccb-b669-8c39ccd833c6\": \"participa_mastermind\",\n  \"a1b00ecc-4d9b-4e53-977a-59a9f2609333\": \"evento_valioso\",\n  \"003dc18b-3f2e-44b4-8c5a-04c45b00c723\": \"necessidade_especial\",\n  \"9743d985-e7bb-4deb-b13f-ed4fcf726fc4\": \"restricao_alimentar\",\n  \"9aed8180-2d5a-4a87-8f7d-8262d7f5f35a\": \"descricao_restricao\",\n  \"42e331bd-1dc2-4148-9555-bcbe729e3e29\": \"disposto_investir\"\n};\n\n// 東 Monta objeto com respostas mapeadas\nlet respostasMapeadas = {};\n\nrespostas.forEach(answer => {\n  const ref = answer.field.ref;\n  const campo = perguntasMap[ref] || ref;\n\n  let valor = \"\";\n\n  switch (answer.type) {\n    case \"boolean\":\n      valor = answer.boolean ? \"Sim\" : \"Nﾃ｣o\";\n      break;\n    case \"choice\":\n      valor = answer.choice?.label || \"\";\n      break;\n    case \"text\":\n    case \"email\":\n    case \"phone_number\":\n      valor = answer[answer.type] || \"\";\n      break;\n    default:\n      valor = \"\";\n  }\n\n  respostasMapeadas[campo] = valor;\n});\n\n// 東 Funﾃｧﾃｵes auxiliares\nfunction capitalizeName(fullName) {\n  if (!fullName) return { fname: \"\", lname: \"\", fullname: \"\" };\n  const lowerWords = [\"da\", \"de\", \"do\", \"di\", \"du\"];\n  let words = fullName.toLowerCase().split(\" \").map((word, idx) =>\n    lowerWords.includes(word) && idx !== 0 ? word : word.charAt(0).toUpperCase() + word.slice(1)\n  );\n  const fname = words.shift();\n  const lname = words.join(\" \");\n  return {\n    fname,\n    lname,\n    fullname: lname ? `${fname} ${lname}` : fname\n  };\n}\n\nfunction formatPhone(phone) {\n  if (!phone) return \"\";\n  const clean = phone.replace(/\\D/g, \"\");\n  const ddi = \"55\";\n  const ddd = clean.slice(2, 4);\n  let numero = clean.slice(4);\n  if (numero.length === 8) numero = `9${numero}`;\n  return `${ddi}${ddd}${numero}`;\n}\n\nfunction formatEmail(email) {\n  return email ? email.toLowerCase().trim() : \"\";\n}\n\nfunction formatInstagram(instagram) {\n  return instagram ? instagram.toLowerCase().replace(/^@/, \"\") : \"\";\n}\n\nfunction formatDate(iso) {\n  if (!iso) return \"\";\n  const date = new Date(iso);\n  const pad = n => String(n).padStart(2, \"0\");\n  return `${date.getFullYear()}-${pad(date.getMonth() + 1)}-${pad(date.getDate())} ${pad(date.getHours())}:${pad(date.getMinutes())}`;\n}\n\n// 東 Dados formatados\nconst formattedNames = capitalizeName(respostasMapeadas.fullName);\nconst formattedEmail = formatEmail(respostasMapeadas.email);\nconst formattedPhone = formatPhone(respostasMapeadas.telefone);\nconst formattedInstagram = formatInstagram(respostasMapeadas.instagram);\nconst dataAplicacaoFormatada = formatDate(formResponse.submitted_at);\n\n// 東 Objeto final\nconst result = {\n  ...formattedNames,\n  email: formattedEmail,\n  whatsapp: formattedPhone,\n  nome_empresa: respostasMapeadas.nome_empresa || \"\",\n  instagram: formattedInstagram,\n  dificuldades_empresa: respostasMapeadas.dificuldades_empresa || \"\",\n  faturamento: respostasMapeadas.faturamento || \"\",\n  metricas_claras: respostasMapeadas.metricas_claras || \"\",\n  tem_conselho: respostasMapeadas.tem_conselho || \"\",\n  participa_mastermind: respostasMapeadas.participa_mastermind || \"\",\n  evento_valioso: respostasMapeadas.evento_valioso || \"\",\n  necessidade_especial: respostasMapeadas.necessidade_especial || \"\",\n  restricao_alimentar: respostasMapeadas.restricao_alimentar || \"\",\n  descricao_restricao: respostasMapeadas.descricao_restricao || \"\",\n  disposto_investir: respostasMapeadas.disposto_investir || \"\",\n  form_id: formResponse.form_id || \"\",\n  form_name: formResponse.definition.title || \"\",\n  data_aplicacao: dataAplicacaoFormatada\n};\n\n// 東 Retorna para o n8n\nreturn [{ json: result }];\n"
      },
